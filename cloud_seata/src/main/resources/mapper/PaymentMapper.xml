<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mashibing.mapper.PayMapper">
    <update id="payment" useGeneratedKeys="true" >
        update payment
        <set>
            account=account-1.00
        </set>
        <where>
            id=#{id}
        </where>
    </update>
</mapper>
